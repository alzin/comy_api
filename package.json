{
    "name": "example-swagger-express-ts",
    "version": "0.0.6",
    "description": "",
    "main": "built/main.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "nodemon src/index.ts",
        "build": "tsc"
    },
    "repository": {
        "type": "git",
        "url":
            "git+https://github.com/olivierlsc/example-swagger-express-ts.git"
    },
    "keywords": [
        "inversify",
        "inversifyjs",
        "swagger",
        "swagger2",
        "swagger-ui",
        "typescript",
        "expressjs",
        "express",
        "api rest",
        "documentation",
        "decorator",
        "generator",
        "swagger.json",
        "json",
        "rest"
    ],
    "author": "Olivier LIN-SI-CHENG",
    "license": "MIT",
    "homepage":
        "https://github.com/olivierlsc/example-swagger-express-ts.git#readme",
    "dependencies": {
        "@types/body-parser": "^1.19.5",
        "@types/lodash": "^4.17.7",
        "body-parser": "^1.20.2",
        "inversify": "^6.0.2",
        "inversify-express-utils": "6.4.6",
        "lodash": "^4.17.21",
        "reflect-metadata": "^0.2.2",
        "swagger-express-ts": "^1.1.0",
        "swagger-ui-dist": "^5.17.14"
    },
    "devDependencies": {
        "@types/chai": "^4.3.17",
        "@types/mocha": "^10.0.7",
        "@types/sinon": "^17.0.3",
        "chai": "^5.1.1",
        "gulp": "^5.0.0",
        "gulp-clean": "^0.4.0",
        "husky": "^9.1.4",
        "mocha": "^10.7.3",
        "nodemon": "^3.1.4",
        "nyc": "^17.0.0",
        "prettier": "3.3.3",
        "pretty-quick": "^4.0.0",
        "rimraf": "^6.0.1",
        "sinon": "^18.0.0",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^5.5.4"
    },
    "nyc": {
        "include": ["src/**/*.ts"],
        "exclude": ["src/index.ts"],
        "extension": [".ts"],
        "require": ["ts-node/register"],
        "reporter": ["html", "cobertura"],
        "sourceMap": true,
        "instrument": true
    },
    "nodemonConfig": {
        "restartable": "rs",
        "watch": ["src/**/*.ts"],
        "ext": "ts",
        "ignore": ["test/*", "wiki/*", "swagger/*", "node_modules/*", "dist/*"],
        "delay": "2500",
        "exec": "ts-node --project tsconfig.json src/index.ts"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
