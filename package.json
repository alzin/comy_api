{
  "name": "starter-templating",
  "version": "0.0.0",
  "scripts": {
    "start": "ts-node --transpile-only src/server.ts",
    "dev": "nodemon --watch src --delay 1 --exec npx ts-node src/server.ts",
    "cpm": "pm2 start src/server.ts --watch ts-node src/server.ts",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build": "tsc -p .",
    "production": "ts-node --transpile-only dist/server.js"
  },
  "dependencies": {
    "@types/swagger-ui-express": "^4.1.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "jest": "^27.5.1",
    "mongoose": "^6.8.0",
    "morgan": "^1.9.1",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/hpp": "^0.2.1",
    "@types/jest": "^27.4.1",
    "@types/mongoose": "^5.5.41",
    "@types/morgan": "^1.7.37",
    "@types/node": "^18.11.15",
    "@types/supertest": "^2.0.8",
    "@types/swagger-jsdoc": "^3.0.2",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2"
  }
}
